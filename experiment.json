{
    "models": [
        "test"
    ],
    "dataset": "mnist",
    "optimizer": "sgd",
    "opt_params": {
        "learning_rate": 0.01,
        "momentum": 0.9,
        "decay": 0.0001
    },
    "loss": "categorical_crossentropy",
    "metrics": [
        "categorical_accuracy",
        "Precision",
        "Recall",
        "TruePositives",
        "FalsePositives",
        "TrueNegatives",
        "FalseNegatives",
        "AUC",
        "f1_score"
    ],
    "initializers": false,
    "epochs": 4,
    "batch": 256,
    "load_state": false,
    "data_augmentation": false,
    "decay": true,
    "type": "conv",
    "dir": "./resultados/",
    "exp":{
        "k-fold": [2],
        "holdout": [10]
    },
    "state_url": "./resultados/state/state_{}.json"
}